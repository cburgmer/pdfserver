#: pdfserver/templates/main.html:17
msgid "Please check your input."
msgstr "Bitte überprüfe deine Eingabe."

#: pdfserver/templates/main.html:25
msgid "There are only maximum {0} pages."
msgstr "Es existieren nur maximal {0} Seiten."

#: pdfserver/templates/main.html:36 pdfserver/templates/main.html:194
msgid "Delete all files"
msgstr "Alle Dateien löschen"

#: pdfserver/templates/main.html:40
msgid "Cancel"
msgstr "Abbrechen"

#: pdfserver/templates/main.html:50 pdfserver/templates/main.html:192
msgid "Delete"
